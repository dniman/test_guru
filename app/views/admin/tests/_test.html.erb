<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>

  <% if user_signed_in? %>
    <td><%= test.questions.size %></td>
    <td><%= link_to t(".show"), admin_test_path(test) %></td>
    <td><%= link_to t(".edit"), edit_admin_test_path(test) %></td>
    <td>
      <%= link_to t(".delete"),
                admin_test_path(test),
                method: :delete,
                data: { confirm: ".confirm" } %>
    </td>
  <% end %>
</tr>
